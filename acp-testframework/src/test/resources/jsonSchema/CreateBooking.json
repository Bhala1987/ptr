          "$ref": "#/definitions/Pricing"
        }
      }
    },
    "Payment": {
      "type": "object",
      "description": "The payment made on the booking. As part of create booking, only successful transactions are sent to EJ systems.",
      "properties": {
        "type": {
          "type": "string",
          "description": "payment type for the transaction - creditcard,debitcard,voucher,bank-payment,creditfile,cash"
        },
        "transactionId": {
          "type": "string",
          "description": "Hybris generated transaction id"
        },
        "externalReference": {
          "type": "string",
          "description": "Payment reference provided by the payment gateway. For non-card payments,this value is generated by hybris."
        },
        "transactionType": {
          "type": "string",
          "description": "Represents different transaction types - AUTHORIZE, REFUND etc. Added for future use in update booking"
        },
        "creditFileReference": {
          "type": "string",
          "description": "Credit file number used for payment."
        },
        "paymentTypeReference": {
          "type": "string",
          "description": "Stores the last 4 digits of the card, voucher code, receipt number"
        },
        "comment": {
          "type": "string",
          "description": "Comment provided by the agent while capturing payment"
        },
        "amount": {
          "$ref": "#/definitions/TotalPrice"
        },
        "exchangeRate": {
          "type": "number",
          "format": "double",
          "description": "Exchange rate if any for the transaction"
        },
        "exchangeValue": {
          "$ref": "#/definitions/TotalPrice"
        }
      }
    },
    "APIS": {
      "type": "object",
      "description": "Advanced Passenger Information",
      "properties": {
        "name": {
          "$ref": "#/definitions/Name"
        },
        "dateOfBirth": {
          "type": "string",
          "format": "date",
          "description": "Passenger date of birth. ISO 8601 Date Format (YYYY-MM-DD)"
        },
        "documentExpiryDate": {
          "type": "string",
          "format": "date",
          "description": "Expiry date of the travel document. ISO 8601 Date Format (YYYY-MM-DD)"
        },
        "documentNumber": {
          "type": "string",
          "description": "Identification number on the travel document",
          "example": "YT7675433"
        },
        "documentType": {
          "type": "string",
          "description": "Document type code from hybris reference data.",
          "example": "PASSPORT"
        },
        "gender": {
          "type": "string",
          "enum": [
            "MALE",
            "FEMALE"
          ],
          "description": "Gender code. The value will match the ones provided in reference data."
        },
        "nationality": {
          "type": "string",
          "description": "Country iso code. The value will match the ones provided in reference data.",
          "example": "GBR"
        },
        "countryOfIssue": {
          "type": "string",
          "description": "Country iso code. The value will match the ones provided in reference data.",
          "example": "GBR"
        }
      },
      "required": [
        "name",
        "dateOfBirth",
        "documentExpiryDate",
        "documentNumber",
        "documentType",
        "gender",
        "nationality",
        "countryOfIssue"
      ]
    },
    "Corporate": {
      "type": "object",
      "description": "The corporate and deal information linked to the booking",
      "properties": {
        "corporateId": {
          "type": "string",
          "description": "Id of the corporate customer"
        },
        "officeId": {
          "type": "string",
          "description": "Office Id associated with the deal apploed on the booking"
        },
        "dealId": {
          "type": "string",
          "description": "Identifier for the deal applied on the booking"
        },
        "discountTier": {
          "type": "string",
          "description": "Identifier for discount tier the deal belongs to"
        },
        "iataNumber": {
          "type": "string",
          "description": "The IATA number associated with the customer"
        },
        "ejContact": {
          "type": "string",
          "description": "ejContact information stored with the deal"
        }
      }
    },
    "OtherInfo": {
      "type": "object",
      "description": "Other booking information like customer ip address that are captured in the booking",
      "properties": {
        "ipAddress": {
          "type": "string",
          "description": "Customer IP address provided by the channel"
        },
        "device": {
          "type": "string",
          "description": "Device used in the booking and is provided by the channel"
        },
        "operatingSystem": {
          "type": "string",
          "description": "Operating system used in the booking and is provided by the channel"
        }
      }
    },
    "Comment": {
      "type": "object",
      "required": [
        "code",
        "description"
      ],
      "description": "A booking comment instance\n",
      "properties": {
        "code": {
          "type": "string",
          "description": "The comment unique code in hybris (ACP).\n",
          "example": "00564545"
        },
        "description": {
          "type": "string",
          "description": "The comment description in hybris (ACP).\n"
        },
        "agentId": {
          "type": "string",
          "description": "Customer service agent identifier. Mandatory for agent desktop booking\n",
          "example": "00767554"
        },
        "dateTime": {
          "type": "string",
          "format": "datetime",
          "description": "Comment creation date/time. ISO 8601 Date-Time Format (YYYY-MM-DDTHH:MM:SS)\n",
          "example": "2016-10-27T12:26:00"
        },
        "updateAgentId": {
          "type": "string",
          "description": "Customer service agent identifier which has made the latest change to the comment. Mandatory for agent desktop booking\n",
          "example": "00767554"
        },
        "updateDateTime": {
          "type": "string",
          "format": "datetime",
          "description": "Comment update date/time. ISO 8601 Date-Time Format (YYYY-MM-DDTHH:MM:SS)\n",
          "example": "2016-10-27T12:26:00"
        },
        "channel": {
          "type": "string",
          "description": "Agent channel responsible for creating the comment.\n",
          "example": "ADCustomerService"
        },
        "commentType": {
          "type": "string",
          "description": "The associated comment type as defined in hybris ACP\n",
          "example": "booking"
        }
      }
    }
  }
