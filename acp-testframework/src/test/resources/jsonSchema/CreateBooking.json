    "TravelInsurancePolicy": {
      "type": "object",
      "description": "A policy document on the product",
      "properties": {
        "insuredAddress": {
          "$ref": "#/definitions/Address"
        },
        "insuredDOB": {
          "type": "string",
          "format": "date",
          "description": "Date of birth of the policy holder. ISO 8601 Date Format (YYYY-MM-DD)"
        },
        "policyType": {
          "type": "string",
          "description": "Identifier to represent policy types provided by the insurance provider.",
          "example": "SINGLE"
        },
        "coverageArea": {
          "type": "string",
          "description": "Identifier to represent the coverage area provided by the insurance provider.",
